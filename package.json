{
  "name": "niconico-desktop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm install --no-optional & dtsm install",
    "copy:views": "cpx \"./src/server/views/**/*.*\" \"./dist/server/views/\"",
    "tsc": "tsc --rootDir ./src --outDir ./dist",
    "webpack:dev": "webpack --config ./webpack/webpack.config.dev.js",
    "webpack:rel": "webpack --config ./webpack/webpack.config.rel.js",
    "build:dev": "npm run tsc && npm run webpack:dev & npm run copy:views",
    "build:rel": "npm run tsc && npm run webpack:rel && npm run copy:views",
    "start:dev": "node-dev ./dist/server/bin/www.js",
    "start": "node ./dist/server/bin/www.js"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ect": "^0.5.9",
    "express": "~4.13.4",
    "material-ui": "^0.15.2",
    "morgan": "~1.7.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-tap-event-plugin": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "cpx": "^1.3.1",
    "dtsm": "^1.1.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "webpack": "^1.13.1"
  }
}
